{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for all windows",
  "windows": ["main", "region-selector"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:event:allow-emit",
    "core:event:allow-listen",
    "core:window:default",
    "core:window:allow-close",
    "core:window:allow-set-focus",
    "opener:default",
    "dialog:default",
    "dialog:allow-open",
    "shell:default"
  ]
}
